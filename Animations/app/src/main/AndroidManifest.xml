        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".zoom_image.SecondZoomActivity"></activity>
        <activity android:name=".zoom_image.ZoomActivity" />
        <activity android:name=".Bottom_Sheet.ModalBottomSheetActivity" />
        <activity android:name=".Bottom_Sheet.PersistentBottomSheetActivity" />
        <activity android:name=".Shared_Element.SecondActivity" />
        <activity android:name=".Shared_Element.SharedActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>